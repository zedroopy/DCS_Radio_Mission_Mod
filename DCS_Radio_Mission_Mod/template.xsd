<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Radios">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" name="aircraft">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="radio">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element maxOccurs="unbounded" name="preset">
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:unsignedInt">
                            <xs:attribute name="nbr" type="xs:unsignedByte" use="required" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="script-nbr" type="xs:unsignedByte" use="required" />
                  <xs:attribute name="designation" type="xs:string" use="required" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="name" type="xs:string" use="required" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>